{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSONPlaceholder User",
  "type": "object",
  "required": ["id", "name", "username", "email", "address", "phone", "website", "company"],
  "properties": {
    "id": { "type": "integer" },
    "name": { "type": "string" },
    "username": { "type": "string" },
    "email": { "type": "string" },
    "address": { "type": "object" },
    "phone": { "type": "string" },
    "website": { "type": "string" },
    "company": { "type": "object" }
  }
}